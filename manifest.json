{
    "manifest_version": 2,
    "name": "Sunaryum",
    "version": "1.0",
    "description": "Open source Sunaryum wallet extension",
    "icons": {
      "48": "icons/icon48.png",
      "96": "icons/icon96.png",
      "128": "icons/icon128.png"
    },
    "browser_action": {
      "default_icon": "icons/icon48.png",
      "default_popup": "popup/main.html",
      "default_title": "Blockchain Wallet"
    },
    "permissions": [
      "storage",
      "clipboardWrite",
      "https://your-blockchain-api-domain.com/*"
    ]
  }